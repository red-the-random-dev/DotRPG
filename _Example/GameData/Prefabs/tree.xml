<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE dotrpg-prefab>

<ObjectPrefab id="tree" useWith="default/topdown">
    <Require>
        <Texture2D id="tree" location="Texture2D/tree"/>
        <Texture2D id="explosion" location="Texture2D/kaboom"/>
        <SoundEffect id="kaboom" location="Sounds/pixel-explosion"/>
    </Require>
    <Prop colliderSize="48,48" static="true" mass="150">
        <Sprite frameRate="12" frameAmount="1">
            <DefaultAnimationSequence local="tree"/>
            <Animation id="explodes" local="explosion" frameAmount="15" />
        </Sprite>
    </Prop>
</ObjectPrefab>